{
  "name": "express-decorated-router",
  "version": "0.2.2",
  "description": "Define Express routes using TypeScript decorators",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch",
    "build": "rimraf dist && tsc",
    "watch": "npm run build -- --watch",
    "lint": "tslint -p ./tsconfig.tslint.json -s node_modules/custom-tslint-formatters/formatters -t grouped",
    "lint:fix": "npm run lint -- --fix",
    "test": "nyc mocha --throw-deprecation --full-trace -r ts-node/register -r source-map-support/register test/**/*.ts"
  },
  "engines": {
    "node": ">=6.0"
  },
  "keywords": [
    "express",
    "decorator",
    "router",
    "decorated",
    "ts",
    "typescript"
  ],
  "homepage": "https://github.com/Alorel/express-decorated-router",
  "bugs": {
    "url": "https://github.com/Alorel/express-decorated-router/issues"
  },
  "author": {
    "name": "Alorel",
    "url": "https://github.com/Alorel",
    "email": "a.molcanovas@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alorel/express-decorated-router.git"
  },
  "license": "MIT",
  "dependencies": {
    "@types/express": "*",
    "lodash": ">=3.7.0 <5.0",
    "tslib": ">=1.6"
  },
  "peerDependencies": {
    "express": ">=4.0 <5.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/lodash": "^4.14",
    "@types/mocha": "^2.2.47",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "custom-tslint-formatters": "^2.1.1",
    "express": "^4.15",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.3",
    "supertest": "^3.0.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
