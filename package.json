{
  "name": "express-decorated-router",
  "version": "0.2.2",
  "description": "Define Express routes using TypeScript decorators",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "update-toc": "doctoc README.md --github",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch",
    "build": "rimraf dist && tsc",
    "watch": "npm run build -- --watch",
    "lint": "tslint -p . -s node_modules/custom-tslint-formatters/formatters -t grouped 'src/**/*.ts'",
    "lint:fix": "npm run lint -- --fix"
  },
  "engines": {
    "node": ">=6.0"
  },
  "keywords": [
    "express",
    "decorator",
    "router",
    "decorated",
    "ts",
    "typescript"
  ],
  "homepage": "https://github.com/Alorel/express-decorated-router",
  "bugs": {
    "url": "https://github.com/Alorel/express-decorated-router/issues"
  },
  "author": {
    "name": "Alorel",
    "url": "https://github.com/Alorel",
    "email": "a.molcanovas@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alorel/express-decorated-router.git"
  },
  "license": "MIT",
  "dependencies": {
    "glob": ">=7.0 <8.0",
    "lodash": ">=3.7.0 <5.0",
    "tslib": ">=1.6"
  },
  "peerDependencies": {
    "express": ">=4.0 <5.0"
  },
  "devDependencies": {
    "@types/express": "^4.0",
    "@types/glob": "^5.0.31",
    "@types/lodash": "^4.14",
    "bluebird": "^3.5.0",
    "coveralls": "^3.0.0",
    "custom-tslint-formatters": "^2.1.1",
    "doctoc": "^1.3.0",
    "express": "^4.15",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  }
}
