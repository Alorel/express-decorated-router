{
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov"
  ],
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "exclude": [
    "**/node_modules/**/*",
    "**/build/**/*",
    "**/test/**/*"
  ],
  "sourceMap": true,
  "instrument": true,
  "cache": true,
  "all": true
}
